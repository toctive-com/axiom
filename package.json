{
  "name": "@toctive/axiom",
  "version": "0.1.2-alpha.1",
  "private": false,
  "description": "Axiom is a backend framework built using TypeScript and implement Express.js functionality from scratch",
  "keywords": [
    "axiom",
    "backend",
    "TypeScript",
    "requests",
    "express"
  ],
  "author": "Sameh A. Elalfi <sameh.elalfi.mail@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toctive-com/axiom.git"
  },
  "bugs": {
    "url": "https://github.com/toctive-com/axiom/issues"
  },
  "homepage": "https://github.com/toctive-com/axiom#readme",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "bin": {
    "axiom": "./dist/cli.js"
  },
  "scripts": {
    "dev": "nodemon ./src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup --config ./tsup.config.ts",
    "build:watch": "tsup --config ./tsup.config.ts --watch",
    "lint": "tsup ./src",
    "prettier": "prettier . --check --ignore-path .prettierignore",
    "prettier:fix": "prettier . --write --ignore-path .prettierignore",
    "eslint": "eslint .",
    "eslint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@types/node": "^20.4.2",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitest/coverage-v8": "^0.33.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^36.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "lerna": "^7.1.1",
    "nodemon": "^3.0.1",
    "prettier": "3.0.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsc": "^2.0.4",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.6.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  },
  "dependencies": {
    "brandi": "^5.0.0",
    "dotenv": "^16.3.1",
    "dotenv-expand": "^10.0.0",
    "http-terminator": "^3.2.0",
    "setprototypeof": "^1.2.0"
  }
}